<?xml version="1.0" encoding="UTF-8"?>

<mvc >
  <default_action_class namespace="/dailyManager/goodsType" class="com.goodsType.GoodsTypeAction">  
	
	  <action name="goodsTypeList" method="getGoodsTypelist">
	 	 <result name="success">goodsTypeList.jsp</result>
	    <result name="error" type="redirect" >error.jsp</result>
	  </action>
	  
	  
	  
	  
	  	  <action name="addGoodsType" method="addGoodsType">
	 	 <result name="success" type="redirect">goodsTypeList.do</result>
